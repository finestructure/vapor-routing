":[{"text":"Memberwise","kind":"identifier"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Memberwise","kind":"identifier"}],"type":"topic","title":"Conversions.Memberwise","url":"\/documentation\/parsing\/conversions\/memberwise","identifier":"doc:\/\/co.pointfree.VaporRouting\/documentation\/Parsing\/Conversions\/Memberwise"}}}